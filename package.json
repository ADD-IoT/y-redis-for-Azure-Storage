{
  "name": "@y/redis",
  "version": "1.5.3",
  "description": "Scalable websockets provider for Yjs using redis",
  "sideEffects": false,
  "type": "module",
  "funding": {
    "type": "GitHub Sponsors ‚ù§",
    "url": "https://github.com/sponsors/dmonad"
  },
  "scripts": {
    "start": "dotenv run node ./bin/server.js",
    "dist": "rm -rf dist && rollup -c && tsc",
    "lint": "standard && tsc",
    "test": "dotenv run node tests/index.js && npm run lint",
    "test-inspect": "node --inspect-brk tests/index.js",
    "preversion": "npm run lint && npm run dist && test -e dist/src/y-websocket.d.ts && test -e dist/y-websocket.cjs"
  },
  "bin": {
    "y-redis-server": "./bin/server.js"
  },
  "files": [
    "dist/*",
    "bin/*",
    "src/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./bin/utils": "./bin/utils.js",
    "./bin/callback": "./bin/callback.js",
    ".": {
      "module": "./src/y-websocket.js",
      "import": "./src/y-websocket.js",
      "require": "./dist/y-websocket.cjs",
      "types": "./dist/src/y-websocket.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yjs/y-redis.git"
  },
  "keywords": [
    "Yjs"
  ],
  "author": "Kevin Jahns <kevin.jahns@protonmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yjs/y-redis/issues"
  },
  "homepage": "https://github.com/yjs/y-redis#readme",
  "standard": {
    "ignore": [
      "/dist",
      "/node_modules"
    ]
  },
  "dependencies": {
    "lib0": "^0.2.88",
    "postgres": "^3.4.3",
    "redis": "^4.6.12",
    "uws": "github:uNetworking/uWebSockets.js#v20.40.0",
    "y-protocols": "^1.0.6"
  },
  "peerDependencies": {
    "yjs": "^13.5.6"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/ws": "^8.5.10",
    "rollup": "^4.9.5",
    "standard": "^17.1.0",
    "typescript": "^5.3.3",
    "ws": "^8.16.0",
    "y-websocket": "^1.5.3"
  }
}
